{
  // ========== Visuals ==========
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorBlinking": "smooth",
  "editor.fontFamily": "Berkeley Mono, Symbols Nerd Font",
  "editor.fontLigatures": false,
  "editor.fontSize": 14,
  "editor.guides.bracketPairs": "active",
  "editor.lineNumbers": "interval",
  "editor.renderWhitespace": "none",
  "editor.renderLineHighlight": "none",
  "editor.bracketPairColorization.enabled": false,
  "window.autoDetectColorScheme": true,
  "workbench.colorTheme": "Vitesse Dark",
  "workbench.editor.tabActionLocation": "left",
  "workbench.fontAliasing": "antialiased",
  "workbench.iconTheme": "catppuccin-mocha",
  "workbench.list.smoothScrolling": true,
  "workbench.preferredLightColorTheme": "Vitesse Light",
  "workbench.preferredDarkColorTheme": "Vitesse Black",
  "workbench.productIconTheme": "icons-carbon",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tree.expandMode": "singleClick",
  "workbench.tree.indent": 10,
  "workbench.layoutControl.enabled": false,
  "workbench.panel.showLabels": true,
  "workbench.editor.enablePreview": false,

  // ========== Editor ==========
  "debug.onTaskErrors": "debugAnyway",
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
  "editor.find.addExtraSpaceOnTop": false,
  "editor.inlineSuggest.enabled": true,
  "editor.inlineSuggest.showToolbar": "always",
  "editor.inlayHints.enabled": "off",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.stickyScroll.enabled": true,
  "editor.hover.sticky": true,
  "editor.accessibilitySupport": "off",
  "editor.minimap.enabled": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.simpleDialog.enable": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.untrackedChanges": "separate",
  "scm.diffDecorationsGutterWidth": 2,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.tabs.enabled": true,
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.value": 5,
  "search.useParentIgnoreFiles": true,
  "search.exclude": {
    "**/*.snap": true,
    "**/*.svg": true,
    "**/.git": true,
    "**/.github": false,
    "**/.nuxt": true,
    "**/.output": true,
    "**/.pnpm": true,
    "**/.vscode": true,
    "**/.yarn": true,
    "**/assets": true,
    "**/bower_components": true,
    "**/dist/**": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/out/**": true,
    "**/package-lock.json": true,
    "**/pnpm-lock.yaml": true,
    "**/public": true,
    "**/temp": true,
    "**/yarn.lock": true,
    "**/CHANGELOG*": true,
    "**/LICENSE*": true
  },
  // ==========  Global Level Config, needs to put in User Settings ==========
  "window.nativeTabs": true, // this is great, macOS only
  "window.title": "${rootName}", // this make tabs more readable
  "window.titleBarStyle": "custom",
  "window.dialogStyle": "custom",
  "extensions.autoUpdate": "onlyEnabledExtensions",
  // ========== TypeScript configs ==========
  "typescript.locale": "zh-TW",
  "typescript.tsserver.watchOptions": {
    "watchFile": "dynamicPriorityPolling"
  },
  "typescript.experimental.expandableHover": true,
  "typescript.experimental.useTsgo": false,
  // ========== Extension configs ==========
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": false,
  "errorLens.enabledDiagnosticLevels": [
    "warning",
    "error"
  ],
  "errorLens.excludeBySource": [
    "cSpell",
    "Grammarly",
    "eslint"
  ],
  "arrayIndexInlay.minLength": 5,
  "svg.pathDataHighlight": false,
  "svg.preview.mode": "svg",
  "color-highlight.markRuler": false,

  // ========== CommentTranslate ==========
  "commentTranslate.hover.string": false,
  "commentTranslate.hover.enabled": true,
  "commentTranslate.hover.variable": false,
  "commentTranslate.multiLineMerge": true,
  "commentTranslate.hover.content": true,
  "commentTranslate.maxTranslationLength": 1000,
  "commentTranslate.targetLanguage": "zh-TW",

  // ========== i18n-ally ==========
  "i18n-ally.autoDetection": false,
  "i18n-ally.displayLanguage": "zh",
  "i18n-ally.ignoredLocales": [],

  // ========== Iconify ==========
  "iconify.annotations": true,
  "iconify.inplace": true,

  // ========== Vue ==========
  "vue.inlayHints.destructuredProps": true,
  "vue.inlayHints.vBindShorthand": true,
  "vue.inlayHints.missingProps": true,
  "vue.autoInsert.bracketSpacing": true,
  "vue.inlayHints.inlineHandlerLeading": true,
  "vue.editor.focusMode": false,

  // ========== Vim ==========
  "vim.easymotion": false,
  "vim.incsearch": false,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.sneak": true,
  "vim.camelCaseMotion.enable": true,
  "vim.foldfix": true,
  "vim.highlightedyank.color": "rgb(0, 201, 255)",
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.textColor": "white",
  "vim.visualModeKeyBindings": [
    {
      "before": [
        ">"
      ],
      "after": [
        ">",
        "g",
        "v"
      ]
    },
    {
      "before": [
        "<"
      ],
      "after": [
        "<",
        "g",
        "v"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "u"
      ],
      "commands": [
        "undo"
      ]
    },
    {
      "before": [
        "<leader>",
        "d"
      ],
      "after": [
        "d",
        "d"
      ]
    },
    {
      "before": [
        "<C-n>"
      ],
      "commands": [
        ":nohl"
      ]
    },
    {
      "before": [
        "<S-cmd-e>"
      ],
      "commands": [
        "workbench.action.focusActiveEditorGroup",
        "workbench.action.toggleSidebarVisibility"
      ]
    },
    {
      "before": [
        "K"
      ],
      "commands": [
        "editor.action.showHover"
      ]
    },
    {
      "before": [
        "g",
        "r"
      ],
      "commands": [
        "references-view.findReferences"
      ]
    },
    {
      "before": [
        "g",
        "y"
      ],
      "commands": [
        "editor.action.goToTypeDefinition"
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
        "n"
      ],
      "commands": [
        "editor.action.rename"
      ]
    },
    {
      "before": [
        "<C-j>"
      ],
      "commands": [
        "editor.action.marker.next",
        "closeMarkersNavigation"
      ],
      "silent": true
    },
    {
      "before": [
        "leader",
        "c"
      ],
      "commands": [
        "smartClicks.trigger"
      ]
    },
    {
      "before": [
        "s"
      ],
      "commands": [
        "flash-vscode.start"
      ]
    },
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    // VS Code new marker @ next occurence
    "<C-d>": true,
    // Copy
    "<C-c>": false,
    // Cut
    "<C-x>": false,
    // Paste
    "<C-v>": false,
    "<C-z>": false,
    "<C-y>": false
  },
  "vim.autoSwitchInputMethod.enable": true,
  "vim.autoSwitchInputMethod.defaultIM": "com.apple.keylayout.ABC",
  "vim.autoSwitchInputMethod.obtainIMCmd": "/opt/homebrew/bin/im-select",
  "vim.autoSwitchInputMethod.switchIMCmd": "/opt/homebrew/bin/im-select com.apple.keylayout.ABC {im}",
  // ========== Eslint ==========
  // ESLint config: https://github.com/antfu/eslint-config
  // Enable the ESlint flat config support
  "eslint.useFlatConfig": true,
  // Auto fix
  "editor.codeActionsOnSave": {
    "source.fixAll": "never",
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never",
  },
  // ESLint config: https://github.com/antfu/eslint-config
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "eslint.quiet": true,
  // Silent the stylistic rules in you IDE, but still auto fix them
  "eslint.rules.customizations": [
    {
      "rule": "style/*",
      "severity": "off"
    },
    {
      "rule": "format/*",
      "severity": "off"
    },
    {
      "rule": "*-indent",
      "severity": "off"
    },
    {
      "rule": "*-spacing",
      "severity": "off"
    },
    {
      "rule": "*-spaces",
      "severity": "off"
    },
    {
      "rule": "*-order",
      "severity": "off"
    },
    {
      "rule": "*-dangle",
      "severity": "off"
    },
    {
      "rule": "*-newline",
      "severity": "off"
    },
    {
      "rule": "*quotes",
      "severity": "off"
    },
    {
      "rule": "*semi",
      "severity": "off"
    }
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "yaml",
    "toml"
  ],
  // I only use Prettier for manually formatting
  "prettier.enable": false,
  "prettier.printWidth": 200,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "unocss.autocomplete.matchType": "fuzzy",
  "unocss.remToPxPreview": true,
  "cSpell.allowCompoundWords": true,
  "cSpell.language": "en,en-US",
  "cSpell.words": [
    "tsdown",
    "bumpp",
    "iconify",
    "unocss",
    "unplugin",
    "unimport",
    "maru",
    "vueuse"
  ],
  "css.lint.hexColorLength": "ignore",
  "githubIssues.issueBranchTitle": "pr/${user}/issue${issueNumberLabel}",
  "githubIssues.workingIssueFormatScm": "#${issueNumberLabel}",
  "githubPullRequests.showPullRequestNumberInTree": true,
  "githubPullRequests.fileListLayout": "tree",
  "gitlens.codeLens.enabled": false,
  "gitlens.menus": {
    "editor": {
      "blame": false,
      "clipboard": true,
      "compare": true,
      "history": false,
      "remote": false
    },
    "editorGroup": {
      "blame": true,
      "compare": false
    },
    "editorTab": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "explorer": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "scm": {
      "authors": true
    },
    "scmGroup": {
      "compare": true,
      "openClose": true,
      "stash": true
    },
    "scmGroupInline": {
      "stash": true
    },
    "scmItem": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": false,
      "stash": true
    }
  },

  // ========== Cursor ==========
  "cursor.composer.shouldAllowCustomModes": true,
  "cursor.composer.shouldAutoSaveNonAgent": true,
  "cursor.composer.showSuggestedFiles": true,
  "cursor.cmdk.autoSelect": true,
  "cursor.diffs.useCharacterLevelDiffs": true,
  "cursor.general.enableShadowWorkspace": true,
  "cursor.general.gitGraphIndexing": "enabled",
  "cursor.composer.shouldShowMarkdownHoverParticipantActions2": true,
  "cursor.preferNotificationsSameAsChat": true,
  "cursor.cpp.enablePartialAccepts": true,
  "cursor.composer.cmdPFilePicker2": true,
  "cursor.semanticSearch.includeCommitsWithFiles": true,
  "cursor.composer.queueMessageDefaultBehavior": "queue",
  "cursor.composer.backspaceRemoveContext": false,
  "cursor.composer.usageSummaryDisplay": "always",
  "cursor.composer.shouldAutoAcceptDiffs": false,
  "cursor.composer.textSizeScale": 0.85,
  "cursor.enable_agent_window_setting": true,
  "cursor.enable_git_worktrees_setting": true,
  "update.releaseTrack": "dev",
  "update.mode": "silentlyApplyOnQuit",

